site_name: "{{ project_name }}"

theme:
  name: "material"
  features:
    - content.code.copy

plugins:
  - autorefs
  - mkdocstrings

markdown_extensions:
  - pymdownx.highlight:
      anchor_linenums: true
      line_spans: __span
      pygments_lang_class: true
  - pymdownx.inlinehilite
  - pymdownx.snippets
  - pymdownx.superfences

nav:
  - {{ project_name }}: index.md
  - Reference: []
    # - reference/{{ root_module_name }}.main.md
{#
vim: syntax=yaml
#}
